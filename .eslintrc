{
    "extends": "airbnb",
    "rules":
    {
        "react/jsx-filename-extension": "off",
        "jsx-a11y/href-no-hash": "off",
        "class-methods-use-this": "off",
        "import/no-extraneous-dependencies": "off",
        "jsx-a11y/anchor-is-valid": ["warn",
        {
            "aspects": ["invalidHref"]
        }],
        "comma-dangle": ["error", "never"]
    }
}
